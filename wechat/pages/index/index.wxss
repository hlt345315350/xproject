/**index.wxss**/
.header-nav{
  width: 100%;
  margin: 15rpx 0 0 0;
}
.flex-nav{
  height: 100px;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-content: center;
}
.flex-nav-item{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.flex-nav-item image{
  width: 70px;
  height: 70px;
}
.split{
  height: 2rpx;
  width: 95%;
  margin: 20rpx 0;
  background: #FF8364;
}
.content{
  width: 100%;
  display: flex;
}
