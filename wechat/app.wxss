/**app.wxss**/
page{
  height: 100%;
}
.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 5rpx;
  box-sizing: border-box;
}
.x-table{
  width: 100%;
  border: 0px solid darkgray;
  font-size: 32rpx;
  color: #606266
}
.x-th{
  width: 40%;
  justify-content: center;
  background: #FDB87D;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.x-tr{
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.x-td{
  width:40%;
  justify-content: center;
  text-align: center;
}
.x-table .stripe{
  background: #FAFAFA;
}

